<script>
    function display(){
        document.getElementById("none-nav-buttons").style.display="block"
       
    }
    let profilepic =document.getElementById("profile-pic");
let inputfile =document.getElementById("inpu-image");
inputfile.onchange= function(){
    profilepic.src=URL.createObjectURL(inputfile.files[0]);
    display.reload()=false;
}
    
    function updatepic(){
document.getElementById("updating-fieldset").style.display="block";
document.getElementById("fieldset-contents-body").style.display="none"
document.getElementById("none-nav-buttons").style.display="none"
document.getElementById("field-admin").style.display="none"
document.getElementById("galleryf").style.display="none"
    }
    function administrationc(){
        document.getElementById("updating-fieldset").style.display="none";
document.getElementById("fieldset-contents-body").style.display="none"
document.getElementById("none-nav-buttons").style.display="none"
document.getElementById("field-admin").style.display="block"
document.getElementById("galleryf").style.display="none"
document.getElementById("registration").style.display="none"
    } 
    function gallery(){
        document.getElementById("updating-fieldset").style.display="none";
document.getElementById("fieldset-contents-body").style.display="none"
document.getElementById("none-nav-buttons").style.display="none"
document.getElementById("field-admin").style.display="none"
document.getElementById("galleryf").style.display="block"
document.getElementById("registration").style.display="none"

    }
    function registrate(){
        document.getElementById("updating-fieldset").style.display="none";
document.getElementById("fieldset-contents-body").style.display="none"
document.getElementById("none-nav-buttons").style.display="none"
document.getElementById("field-admin").style.display="none"
document.getElementById("galleryf").style.display="none"
document.getElementById("registration").style.display="block"

    }
    var input=document.getElementById("input-f-name").value;

    function check(){
if(input==""){
    alert("reba hejuru wasimbutse")
    
}
else{
    alert("those are your code 0404")
    
}
    }
  ;
  setTimeout(()=>{
    document.getElementById("body2-image").style.display="block"
    document.getElementById("body-image").style.display="none"
    document.getElementById("body3-image").style.display="block"
  },4000)
  ;
  
  
  
</script>
